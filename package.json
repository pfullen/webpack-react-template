    "scheduler": "0.20.1",
    "protobufjs": "6.8.9",
    "@types/react": "17.0.14",
    "@types/react-dom": "17.0.14"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test:coverage",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --cache --cache-location=node_modules/.cache --fix"
    ],
    "*.{sass}": [
      "stylelint --cache --cache-location=node_modules/.cache --fix"
    ],
    "*.{js,jsx,scss,sass}": [
      "prettier --write"
    ]
  }
}
