    "react-intersection-observer": "^9.4.0",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "rxjs": "^7.1.0",
    "scheduler": "^0.20.1",
    "stream-browserify": "^3.0.0"
  },
  "resolutions": {
    "tmpl": "1.0.5",
