{
  "name": "webpack-template",
  "version": "1.31.0",
  "private": true,
  "description": "webpack react template",
  "scripts": {
    "start:win": "webpack serve",
    "build:win": "webpack",
    "start": "webpack  serve --config config/webpack.dev.js",
    "watch": "webpack --watch",
    "build": "NODE_ENV=production webpack --config config/webpack.prod.js ",
    "build:analyze": "NODE_ENV=production webpack --profile --json > stats.json --config ./config/webpack.prod.js",
    "build:tool": "npx webpack-bundle-analyzer ./stats.json dist/",
    "lint": "eslint src --ext ts,js,tsx,jsx --fix",
    "lint:es": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "pipe-line-build": "npm run build",
    "pipeline-build": "npm ci --no-optional && npm run build ",
    "pipeline-publish": "npm publish",
    "pipeline-publish-dev": "npm publish --tag dev",
    "preinstall": "npx npm-force-resolutions",
    "commit": "npx git-cz commit",
    "build-dev": "webpack",
    "clean": "rm -rf ./dist",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:watch": "jest --coverage --watchAll",
    "update-npm-versions": "npm-check --skip-unused --update"
  },
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/core": "7.16.5",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-transform-react-jsx": "7.14.9",
    "@babel/preset-env": "7.16.5",
    "@babel/preset-react": "7.16.5",
    "@babel/preset-typescript": "7.16.7",
    "@emotion/babel-plugin": "11.7.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@svgr/webpack": "5.5.0",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.0.0",
    "@testing-library/react-hooks": "7.0.2",
    "@types/jest": "29.2.3",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.14",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.3",
    "bundle-loader": "0.5.6",
    "canvas": "^2.9.3",
    "clean-webpack-plugin": "^3.0.0",
    "clsx": "1.0.4",
    "core-js": "3.18.0",
    "crypto-browserify": "3.12.0",
    "css-loader": "^5.0.1",
    "cypress": "7.7.0",
    "eslint": "7.30.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-chai-friendly": "0.7.1",
    "eslint-plugin-cypress": "2.11.3",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-testing-library": "^5.9.1",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "^5.0.0-beta.5",
    "http-proxy-middleware": "2.0.1",
    "husky": "4.2.5",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.0.0",
    "jest-svg-transformer": "1.0.0",
    "lint-staged": "11.2.6",
    "mini-css-extract-plugin": "2.4.5",
    "npm-check": "5.9.2",
    "postcss": "^8.2.4",
    "postcss-loader": "^4.1.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "2.3.2",
    "react-refresh": "^0.9.0",
    "sass": "1.45.0",
    "sass-loader": "^10.1.1",
    "tmpl": "1.0.5",
    "ts-jest": "26.0.0",
    "typescript": "4.8.4",
    "webpack": "5.65.0",
    "webpack-bundle-analyzer": "4.5.0",
